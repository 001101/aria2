msgid "CUID#%d - Good checksum: %s"
msgstr "CUID#%d - Правильна контрольна сума: %s"

#: src/message.h:62
#, c-format
msgid "CUID#%d - Bad checksum: %s"
msgstr "CUID#%d - Неправильна контрольна сума: %s"


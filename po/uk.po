msgid "CUID#%d - Connecting to the peer %s"
msgstr "CUID#%d - З'єднання із хостом %s"

#: src/message.h:69
#, c-format
msgid ""
"CUID#%d - Piece received. index=%d, begin=%d, length=%d, offset=%llu, "
"blockIndex=%d"
msgstr ""
"CUID#%d - Отримано сегмент. Індекс=%d, початок=%d, довжина=%d, відступ=%llu, "

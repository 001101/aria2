msgid "CUID#%d - we got wrong piece. index=%d"
msgstr "CUID#%d - Отримана неправильна частина. Індекс=%d"

#: src/message.h:59
#, c-format
msgid "CUID#%d - Download not complete: %s"
msgstr "CUID#%d - Завантаження не закінчено: %s"

#: src/message.h:60

#: src/main.cc:95
#, c-format
msgid "Contact Info: %s\n"
msgstr "Связь с автором: %s\n"

#: src/main.cc:101
#, c-format
msgid "Usage: %s [options] URL ...\n"
msgstr "Использование: %s [параметры] URL ...\n"

#: src/main.cc:103
#, c-format
msgid "       %s [options] -T TORRENT_FILE FILE ...\n"

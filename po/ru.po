msgid "                              Default: "
msgstr "                              По умолчанию: "

#: src/OptionHandler.cc:39
msgid "                              Tags: "
msgstr "                              Метки: "

#: src/OptionHandler.cc:40
msgid "                              Possible Values: "
msgstr "                              Возможные значения: "


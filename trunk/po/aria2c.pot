#: src/main.cc:160
msgid "Options:"
msgstr ""

#: src/main.cc:161
msgid " -d, --dir=DIR                The directory to store downloaded file."
msgstr ""

#: src/main.cc:162
msgid " -o, --out=FILE               The file name for downloaded file."
msgstr ""

#: src/main.cc:163
msgid ""
" -l, --log=LOG                The file path to store log. If '-' is "
"specified,\n"
"                              log is written to stdout."
msgstr ""

#: src/main.cc:165
msgid " -D, --daemon                 Run as daemon."
msgstr ""

#: src/main.cc:166
msgid ""
" -s, --split=N                Download a file using N connections. N must "
"be\n"
"                              between 1 and 5. This option affects all "
"URLs.\n"
"                              Thus, aria2 connects to each URL with\n"
"                              N connections."
msgstr ""

#: src/main.cc:170
msgid ""
" --retry-wait=SEC             Set amount of time in second between requests\n"
"                              for errors. Specify a value between 0 and 60.\n"
"                              Default: 5"
msgstr ""

#: src/main.cc:173
msgid " -t, --timeout=SEC            Set timeout in second. Default: 60"
msgstr ""

#: src/main.cc:174
msgid ""
" -m, --max-tries=N            Set number of tries. 0 means unlimited.\n"
"                              Default: 5"
msgstr ""

#: src/main.cc:176
msgid ""
" --min-segment-size=SIZE[K|M] Set minimum segment size. You can append\n"
"                              K or M(1K = 1024, 1M = 1024K). This\n"
"                              value must be greater than or equal to\n"
"                              1024."
msgstr ""

#: src/main.cc:180
msgid ""
" --http-proxy=HOST:PORT       Use HTTP proxy server. This affects to all\n"
"                              URLs."
msgstr ""

#: src/main.cc:182
msgid " --http-user=USER             Set HTTP user. This affects to all URLs."
msgstr ""

#: src/main.cc:183
msgid ""
" --http-passwd=PASSWD         Set HTTP password. This affects to all URLs."
msgstr ""

#: src/main.cc:184
msgid ""
" --http-proxy-user=USER       Set HTTP proxy user. This affects to all URLs"
msgstr ""

#: src/main.cc:185
msgid ""
" --http-proxy-passwd=PASSWD   Set HTTP proxy password. This affects to all "
"URLs."
msgstr ""

#: src/main.cc:186
msgid ""
" --http-proxy-method=METHOD   Set the method to use in proxy request.\n"
"                              METHOD is either 'get' or 'tunnel'.\n"
"                              Default: tunnel"
msgstr ""

#: src/main.cc:189
msgid ""
" --http-auth-scheme=SCHEME    Set HTTP authentication scheme. Currently, "
"basic\n"
"                              is the only supported scheme.\n"
"                              Default: basic"
msgstr ""

#: src/main.cc:192
msgid " --referer=REFERER            Set Referer. This affects to all URLs."
msgstr ""

#: src/main.cc:193
msgid ""
" --ftp-user=USER              Set FTP user. This affects to all URLs.\n"
"                              Default: anonymous"
msgstr ""

#: src/main.cc:195
msgid ""
" --ftp-passwd=PASSWD          Set FTP password. This affects to all URLs.\n"
"                              Default: ARIA2USER@"
msgstr ""

#: src/main.cc:197
msgid ""
" --ftp-type=TYPE              Set FTP transfer type. TYPE is either "
"'binary'\n"
"                              or 'ascii'.\n"
"                              Default: binary"
msgstr ""

#: src/main.cc:200
msgid " -p, --ftp-pasv               Use passive mode in FTP."
msgstr ""

#: src/main.cc:201
msgid ""
" --ftp-via-http-proxy=METHOD  Use HTTP proxy in FTP. METHOD is either 'get' "
"or\n"
"                              'tunnel'.\n"
"                              Default: tunnel"
msgstr ""

#: src/main.cc:205
msgid " -T, --torrent-file=TORRENT_FILE  The file path to .torrent file."
msgstr ""

#: src/main.cc:206
msgid ""
" --follow-torrent=true|false  Setting this option to false prevents aria2 "
"to\n"
"                              enter BitTorrent mode even if the filename of\n"
"                              downloaded file ends with .torrent.\n"
"                              Default: true"
msgstr ""

#: src/main.cc:210
msgid ""
" -S, --show-files             Print file listing of .torrent file and exit."
msgstr ""

#: src/main.cc:211
msgid ""
" --direct-file-mapping=true|false Directly read from and write to each file\n"
"                              mentioned in .torrent file.\n"
"                              Default: true"
msgstr ""

#: src/main.cc:214
msgid ""
" --listen-port=PORT           Set port number to listen to for peer "
"connection."
msgstr ""

#: src/main.cc:215
msgid ""
" --upload-limit=SPEED         Set upload speed limit in KB/sec. aria2 tries "
"to\n"
"                              keep upload speed under SPEED. 0 means "
"unlimited."
msgstr ""

#: src/main.cc:217
msgid ""
" --select-file=INDEX...       Set file to download by specifing its index.\n"
"                              You can know file index through --show-files\n"
"                              option. Multiple indexes can be specified by "
"using\n"
"                              ',' like \"3,6\".\n"
"                              You can also use '-' to specify rangelike \"1-5"
"\".\n"
"                              ',' and '-' can be used together."
msgstr ""

#: src/main.cc:224
msgid " -v, --version                Print the version number and exit."
msgstr ""

#: src/main.cc:225
msgid " -h, --help                   Print this message and exit."
msgstr ""

#: src/main.cc:228
msgid ""
" You can specify multiple URLs. All URLs must point to the same file\n"
" or downloading fails."
msgstr ""

#: src/main.cc:233
msgid ""
" Specify files in multi-file torrent to download. Use conjunction with\n"
" -T option. This arguments are ignored if you specify --select-file option."
msgstr ""

